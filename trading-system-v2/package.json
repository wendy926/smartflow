{
  "name": "trading-system-v2",
  "version": "2.0.0",
  "description": "基于V3和ICT策略的高性能交易系统",
  "main": "src/main.js",
  "scripts": {
    "start": "node src/main.js",
    "dev": "nodemon src/main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:status": "pm2 status",
    "db:migrate": "node scripts/database/migrate.js",
    "db:seed": "node scripts/database/seed.js",
    "db:clean": "node scripts/database/clean.js",
    "deploy": "bash scripts/deploy.sh",
    "monitor": "node src/workers/monitor.js"
  },
  "keywords": [
    "trading",
    "strategy",
    "v3",
    "ict",
    "binance",
    "cryptocurrency"
  ],
  "author": "Trading System Team",
  "license": "MIT",
  "dependencies": {
    "chart.js": "^4.4.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron": "^2.3.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.2",
    "node-telegram-bot-api": "^0.64.0",
    "pm2": "^5.3.0",
    "socket.io": "^4.7.2",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "axios": "^1.12.2",
    "eslint": "^8.47.0",
    "jest": "^29.6.2",
    "mysql2": "^3.15.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "redis": "^5.8.2",
    "supertest": "^6.3.3",
    "ws": "^8.18.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}

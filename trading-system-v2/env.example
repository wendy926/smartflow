# 环境变量配置文件
# 复制此文件为 .env 并填入实际值

# 数据库配置
MYSQL_ROOT_PASSWORD=your_mysql_root_password
MYSQL_PASSWORD=your_mysql_password
REDIS_PASSWORD=your_redis_password
MONGODB_PASSWORD=your_mongodb_password
INFLUXDB_PASSWORD=your_influxdb_password

# AI服务配置
CLAUDE_API_KEY=your_claude_api_key
DEEPSEEK_API_KEY=your_deepseek_api_key
OPENAI_API_KEY=your_openai_api_key

# 市场API配置
BINANCE_API_KEY=your_binance_api_key
BINANCE_SECRET_KEY=your_binance_secret_key

# 美股API配置
ALPACA_API_KEY=your_alpaca_api_key  # 仅用于获取市场数据，可选
ALPHA_VANTAGE_API_KEY=6XV08K479PGSITYI  # 已提供，用于期权、VIX等数据

# A股API配置 (Tushare Pro)
TUSHARE_TOKEN=your_tushare_token_here  # 在 https://tushare.pro/ 获取

# A股模拟交易配置
CN_STOCK_SIMULATION_MODE=true  # 仅模拟交易，不真实下单
CN_STOCK_INDEX_SYMBOLS=000300.SH,000905.SH,000852.SH,399001.SZ,399006.SZ  # 默认指数列表

# 券商API配置（如需真实交易，暂不使用）
# BROKER_API_URL=your_broker_api_url
# BROKER_API_KEY=your_broker_api_key
# BROKER_SECRET_KEY=your_broker_secret_key

# 监控配置
GRAFANA_ADMIN_PASSWORD=your_grafana_admin_password

# 系统配置
NODE_ENV=production
REGION=SG
CONFIG_PATH=./config
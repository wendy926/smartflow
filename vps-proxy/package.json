{
  "name": "smartflow-vps-proxy",
  "version": "1.0.0",
  "description": "VPS 代理服务器 - 新加坡中转 Binance API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "pm2": "pm2 start server.js --name smartflow-proxy",
    "pm2:stop": "pm2 stop smartflow-proxy",
    "pm2:restart": "pm2 restart smartflow-proxy",
    "pm2:logs": "pm2 logs smartflow-proxy",
    "pm2:status": "pm2 status"
  },
  "dependencies": {
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "cors": "^2.8.5",
    "express-rate-limit": "^6.7.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "proxy",
    "binance",
    "api",
    "trading",
    "smartflow"
  ],
  "author": "SmartFlow Team",
